<h1>extraHabitacion</h1>


<% if (result.length == 0) { %>
  <h4>
    0 extraHabitacion
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>id_extra_habitacion</th>
			<th>jacuzzi</th>
			<th>vista_al_mar</th>
			<th>costo_extra</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.id_extra_habitacion %>
					</td>
					<td>
						<%= item.jacuzzi %>
					</td>
					<td>
						<%= item.vista_al_mar %>
					</td>
					<td>
						<%= item.costo_extra %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/extraHabitacion/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/extraHabitacion/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/extraHabitacion/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/extraHabitacion/create">New</a>