<div class="">
  <h1>New menu</h1>

  <div class="body">
    <form class="" action="/menu/update" method="post">
      <input name="id" value="<%= element.id %>" hidden="true" required>
        <div class="">
          <label for="">id_menu</label>
          <input type="number" name="id_menu" value="<%= element.id_menu %>" required="false">
        </div>
        <div class="">
          <label for="">desc_menu</label>
          <input type="text" name="desc_menu" value="<%= element.desc_menu %>" required="false">
        </div>
      <button type="submit">Update menu</button>
    </form>
    <br />
    <a href="/menu">Index</a>
  </div>
</div>