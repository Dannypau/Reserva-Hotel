<h1>cliente</h1>


<% if (result.length == 0) { %>
  <h4>
    0 cliente
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>id_cliente</th>
			<th>nombre_cliente</th>
			<th>correo</th>
			<th>direccion</th>
			<th>telefono</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.id_cliente %>
					</td>
					<td>
						<%= item.nombre_cliente %>
					</td>
					<td>
						<%= item.correo %>
					</td>
					<td>
						<%= item.direccion %>
					</td>
					<td>
						<%= item.telefono %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/cliente/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/cliente/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/cliente/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/cliente/create">New</a>