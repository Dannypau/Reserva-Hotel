
<div class="container">
<div class="col-sm-3"></div>
    <div class="signup-form-container col-sm-6 opaque1">

<h1>Perfil Personal</h1>


<% if (result.length == 0) { %>
  <h4>
    0 cliente
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>dni</th>
			<th>nombre_cliente</th>
			<th>correo</th>
			<th>telefono</th>
			<th>id</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.dni %>
					</td>
					<td>
						<%= item.nombre_cliente %>
					</td>
					<td>
						<%= item.correo %>
					</td>
					<td>
						<%= item.telefono %>
					</td>
					<td>
						<%= item.id %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/cliente/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/cliente/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/cliente/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/cliente/create">New</a>
    </div>
</div>